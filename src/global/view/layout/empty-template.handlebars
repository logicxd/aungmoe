            // Add other bootstrap parameters as needed, using camel case.
        });
    </script>
    {{/if}}
    <script src="/js/_footer.js" ></script>
    <script>
        $(document).ready(() => {
            let year = (new Date()).getFullYear()
            $('#copyright-year').text(year)
        })
    </script>
</body>
</html>
